{
  "name": "@azuliani/deep-diff",
  "version": "1.0.0",
  "description": "Library for computing structural differences between JavaScript objects",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "engines": {
    "node": ">=23.6.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:esm && npm run build:cjs && npm run build:cjs-package",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs-package": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "test": "node --test test/*.test.ts",
    "test:coverage": "node --test --experimental-test-coverage test/*.test.ts",
    "bench": "npm run build && node bench/run.js",
    "bench:gc": "npm run build && node --expose-gc bench/run.js",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "docs": "typedoc --out docs src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azuliani/deep-diff.git"
  },
  "keywords": [
    "diff",
    "deep-diff",
    "object",
    "compare",
    "difference"
  ],
  "author": "azuliani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azuliani/deep-diff/issues"
  },
  "homepage": "https://github.com/azuliani/deep-diff#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typedoc": "^0.27.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
